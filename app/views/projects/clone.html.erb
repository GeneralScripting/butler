<% content_for(:breadcrumb) do %>
  <%= breadcrumb_box do %>
    <%= link_to "Projects", projects_path %>
    <span class="divider">&raquo;</span>
    Clone <%= link_to @original.name, project_path(@original) %>
  <% end %>
<% end %>

<% content_for(:page_title) do %>
  <% @page_title = "Clone project #{@original.name}" %>
  <h1>Clone project</h1>
<% end %>

<%= form_for(:project, :url => clone_path(@original)) do |f| %>
  <%= render(:partial => 'form')%>
  <div class="form-actions">
    <%= f.submit "Update project", class: "btn btn-primary" %>
    <%= link_to 'Back to project', projects_path, :class => 'btn' %>
  </div>
<% end %>
